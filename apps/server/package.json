{
  "name": "@mativated-monorepo/server",
  "type": "module",
  "version": "0.0.0",
  "description": "TypeScript + Prisma + MySQL + Express API Server",
  "author": "",
  "license": "ISC",
  "scripts": {
    "server": "npm run build && cross-env NODE_ENV=production pm2 start src/run-ts.sh",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "swc src -d dist --source-maps --copy-files",
    "ts": "tsc && tsc-alias",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "prisma:init": "prisma init",
    "prisma:migrate": "prisma migrate dev --preview-feature",
    "prisma:generate": "prisma generate",
    "prisma:seed": "npx tsx src/prisma/seed.ts",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only prod",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^4.13.9",
    "@prisma/client": "^5.10.2",
    "@trpc/server": "11.0.0-next-beta.294",
    "axios": "^1.7.9",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.3",
    "envalid": "^7.3.1",
    "express": "^4.18.1",
    "hpp": "^0.2.3",
    "reflect-metadata": "^0.1.13",
    "svix": "^1.18.0",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.22.4",
    "zod-prisma-types": "^3.1.6"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.220",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.1",
    "node-config": "^0.0.2",
    "node-gyp": "^9.1.0",
    "nodemon": "^2.0.19",
    "pm2": "^5.2.0",
    "prettier": "^2.7.1",
    "prisma": "^5.9.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "tsc-alias": "^1.7.0",
    "tsx": "^4.7.2",
    "typescript": "^5.3.3"
  }
}
