{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"],
      "components/*": ["./components/*"],
      "pages/*": ["./pages/*"],
      "assets/*": ["./assets/*"],
      "lib/*": ["./lib/*"],
      "routes/*": ["./routes/*"],
      "utils/*": ["./utils/*"],
      "@trpc/server/*": ["../server/node_modules/@trpc/server/*"]
    }
  },
  "include": ["src"],
  "references": [
    {
      "path": "./tsconfig.node.json"
    },
    {
      "path": "../server"
    }
  ]
}